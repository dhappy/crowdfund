%p#notice= notice

%ul
  %li= "Name: #{@organization.name}"
  - if @organization.projects.any?
    %li
      Projects:
      %ul
        - @organization.projects.each do |project|
          - @project = project
          %li= render 'projects/table'

= link_to 'Edit', edit_organization_path(@organization)
\|
= link_to 'Back', organizations_path
